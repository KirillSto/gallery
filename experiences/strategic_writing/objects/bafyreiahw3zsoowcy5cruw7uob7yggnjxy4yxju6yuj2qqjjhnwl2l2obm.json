            "649da420ccba0ab5dfe16589",
            "649da420ccba0ab5dfe1658a"
          ],
          "text": {
            "text": "1) Organizing \u0026 Accessing Objects: ",
            "marks": {
              "marks": [
                {
                  "range": {
                    "to": 35
                  },
                  "type": "Bold"
                }
              ]
            }
          }
        },
